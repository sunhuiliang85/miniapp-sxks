

@font-face {
    font-family: 'more-sign';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('more-sign.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

[class^="more-sign"], [class*=" more-sign"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'more-sign' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
  
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  
  .more-signpunch-tick:before {
    content: "\e900";
  }
  