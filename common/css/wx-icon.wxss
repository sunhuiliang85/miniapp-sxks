@font-face {
  font-family: 'wx-font';
  src: url(data:application/font-ttf;charset=utf-8;base64,);
  font-weight: normal;
  font-style: normal;
}

[class^="wx-icon-"], [class*=" wx-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'wx-font' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.wx-icon-checkbox:before {
  content: "\e900";
}
.wx-icon-tick:before {
  content: "\e902";
}